    "editor.multiCursorModifier": "ctrlCmd",
    "blade.format.enable": true,
    "emmet.triggerExpansionOnTab": true,
    "editor.snippetSuggestions": "top",
    "files.insertFinalNewline": false,
    "files.trimFinalNewlines": true,
    "[blade]": {
        "editor.defaultFormatter": "shufo.vscode-blade-formatter"
    },
    "workbench.editor.wrapTabs": true,
    "workbench.editor.splitInGroupLayout": "vertical",
    "editor.inlayHints.fontFamily": "Inter",
    "editor.inlineSuggest.fontFamily": "Inter",
    "php.validate.executablePath": "C:\\laragon\\bin\\php\\php-8.2\\php.exe",
    "terminal.external.windowsExec": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe", // enable tab to expanse emmet tags
    "terminal.integrated.automationProfile.windows": {
        "path": "C:\\Users\\sysad\\Documents\\WindowsPowerShell\\Microsoft.Powershell_profile",
    },
    "window.title": "${rootName}${separator}${dirty}${activeEditorShort}${separator}${appName}",
    "workbench.activityBar.location": "top",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar"
    },
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "git.autofetch": true,
    "editor.guides.bracketPairs": true,
    "terminal.integrated.fontSize": 14,
    "workbench.editor.tabSizing": "shrink",
    "vscord.app.name": "Visual Studio Code",
    "vscord.app.privacyMode.enable": true,
    "vscord.status.details.text.viewing": "Code in {LANG}",
    "vscord.status.details.text.editing": "Code in {LANG}",
    "workbench.colorTheme": "GitLab Dark",
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.stickyScroll.maxLineCount": 2,
    "editor.inlineSuggest.suppressSuggestions": true,
    "workbench.startupEditor": "none",
    "terminal.integrated.fontWeight": "bold"
